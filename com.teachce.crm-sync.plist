<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.teachce.crm-sync</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/timothysepulvado/Teach/CRM_integration/sync.sh</string>
        <string>full</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/timothysepulvado/Teach/CRM_integration</string>

