# Kajabi API Configuration
KAJABI_API_KEY=your_kajabi_client_id_here
KAJABI_API_SECRET=your_kajabi_client_secret_here

# HubSpot API Configuration
HUBSPOT_ACCESS_TOKEN=your_hubspot_personal_access_token_here

# Additional Configuration
# Only process purchases created after this date/time (ISO 8601 format in PST)
# Set to 2026-01-05 00:00:00 PST (which is 2026-01-05 08:00:00 UTC)
PURCHASE_CUTOFF_DATE=2026-01-05T08:00:00Z

# Optional: Poll interval in minutes for scheduler
POLL_INTERVAL=5
