    <key>StandardOutPath</key>
    <string>/Users/timothysepulvado/Teach/CRM_integration/logs/launchd.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/timothysepulvado/Teach/CRM_integration/logs/launchd_error.log</string>

